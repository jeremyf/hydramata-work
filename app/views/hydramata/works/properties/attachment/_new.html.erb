<div class="<%= new.dom_class %> form-group">
  <%= label_tag(new.dom_id_for_field, new.label, new.dom_label_attributes) %>
  <div class="values">
    <%= file_field_tag(
      new.dom_id_for_field,
      new.dom_input_attributes(class: 'blank-input', multiple: true)
    ) %>
  </div>
  <% new.with_text_for(:help) do |help| %>
    <p class="help-block"><%= help %></p>
  <% end %>
</div>
